---
layout: null
sitemap: false
---
<?xml version="1.0" encoding="utf-8"?>
<search>
  {% for post in site.posts %}
    <entry>
      <title><![CDATA[{{ post.title | strip_html }}]]></title>
      <url>{{ post.url | relative_url | xml_escape }}</url>

      {% assign cleaned_content = post.content | strip_html | strip_newlines | replace: '', '' | replace: '', '' | replace: '', '' | replace: '', '' %}
      {% if cleaned_content contains '' %}
        <!-- bbbbbbb -->
      {% endif %}
      <content type="text"><![CDATA[{{ cleaned_content }}]]></content>
      <categories>
        {% for category in post.categories %}
          <category> {{ category | strip_html | xml_escape }} </category>
        {% endfor %}
      </categories>
      <tags>
        {% for tag in post.tags %}
          <tag> {{ tag | strip_html | xml_escape }} </tag>
        {% endfor %}
      </tags>
      <tags></tags>
    </entry>
  {% endfor %}
</search>
